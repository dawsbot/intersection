{
	"name": "Intergenre",
	"version": "0.0.0",
	"manifest_version": 2,
	"description": "Providing genre recognition to the internet at large via a Chrome Extension.",
	"browser_action": {
		"default_icon": "Icon.png",
		"default_title": "Intergenre",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["content.js"],
			"run_at": "document_end"
		}
	]
}
